    <a href="#">Users</a>
    <a href="#">Projects</a>
    <a href="#">Departments</a>
    <a href="#">Professors</a>
    <a href="#">AboutUs</a>
  </div>
  
  <div class="right"> 
    <a href="/logout">Logout</a>
  </div>

</div>

<table id="t01">
  <tr>
    <th>Name</th>
    <th>Age</th> 
    <th>Email</th>
  </tr>
  {{#each data}}
  <tr>
    <td>{{this.name}}</td>
    <td>{{this.age}}</td>
    <td>{{this.email}}</td>
  </tr>
  {{/each}}
  <tr>
</table>

    
